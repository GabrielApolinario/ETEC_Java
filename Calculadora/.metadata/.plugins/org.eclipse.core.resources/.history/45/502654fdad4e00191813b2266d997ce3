import javax.swing.JOptionPane;

public class Testes {

	public static void main(String[] args) {

		int Valor;

		Valor = Integer.parseInt(JOptionPane.showInputDialog("Insira um Valor"));


		if (Valor <= 10) {System.out.println("Este número é menor ou igual a 10!");

		} else if (Valor >= 10 && Valor <= 20) {System.out.println("Este número está entre 10 e 20!");

		} else if (Valor >= 20 && Valor <=30) {System.out.println("Este número está entre 20 e 30!");

		} else if (Valor >30) {System.out.println("Este número é maior que 30!");

		}






	}

}