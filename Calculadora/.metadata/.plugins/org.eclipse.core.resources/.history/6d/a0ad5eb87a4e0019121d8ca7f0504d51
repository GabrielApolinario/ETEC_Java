import javax.swing.JOptionPane;

public class Testes {

	public static void main(String[] args) {

int Valor;

 	Valor = Integer.parseInt(JOptionPane.showInputDialog("Insira um Valor"));
 
 	
 		if (Valor <= 10) {System.out.println("Este número é menor ou igual a 10!");
	 
 		} else if (Valor >= 11) {System.out.println("Este número é maior ou igual a 11!");
 
 		}
 
	
	
	
		
		
}
	
}